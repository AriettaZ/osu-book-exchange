<h1>Order #<%=@order.id%></h1>

<%= render 'showOrder', order: @order, showFrom: @showFrom %>
<% if @showFrom == "order_index"%>
  <%= link_to 'Back', orders_path %>
<%elsif @showFrom == "contract_index"%>
  <%= link_to 'Back', url_for(:back) %>
<% elsif @showFrom == "mycontract"%>
  <%= link_to 'Back', profile_mycontract_path%>
<%end%>
